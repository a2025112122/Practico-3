#include <stdio.h>

int main() {
    int N;        
    int sum=0; 
    printf("Introduce un numero N: ");
    scanf("%d", &N);

    if (N <= 1) {
        printf("El número debe ser mayor o igual a 1.\n");
        return 0; 
    }
    for (int i = 1; i <= N; i++) {
        sum+= i;  
    }
    printf("La suma de los números del 1 al %d es: %d\n", N, sum);
    return 0;
}
