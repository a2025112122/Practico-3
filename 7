#include <stdio.h>
int main() {
    int fact=1, N; 
    printf("Introduce un numero N: ");
    scanf("%d", &N);
if(N<0){
    printf("no hay factorial de un num negativo.\n");
}
else{
    for (int i = 1; i <= N; i++) {
            fact*= i;
        }

printf("El factorial de %d es: %d\n", N, fact);
}
    return 0;
}
